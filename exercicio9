#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char nome[40],sexo;
int idade,i;
int main (){
for(i=1;i<=20;i++){
printf("Digite seu nome: ");
scanf("%s",&nome);
printf("Digite sua idade: ");
scanf("%i",&idade);
printf("Digite seu sexo (m para masculino, f para feminino, o para outro): ");
scanf("%s",&sexo);
if(idade>21 && sexo=='m'){
    printf("-->Seu nome é %s, seu sexo é masculino e sua idade é %i \n",nome,idade);
    }
    
    }
}
